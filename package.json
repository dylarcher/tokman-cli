{
  "author": "Dylan Archer <dylarcher@gmail.com> (@dylarcher)",
  "bin": {
    "cssman": "./src/cli.js"
  },
  "browser": "dist/index.umd.js",
  "bugs": {
    "url": "https://github.com/dylarcher/cssman-cli/issues"
  },
  "description": "A design token generator library with CLI support.",
  "dependencies": {
    "yargs": "18.0.0",
    "commander": "14.0.0",
    "css-tree": "3.1.0"
  },
  "devDependencies": {
    "@types/node": "22.15.30",
    "@types/yargs": "17.0.33"
  },
  "directories": {
    ".github": ".github",
    "dist": "dist",
    "docs": "docs",
    "lib": "lib",
    "node_modules": "node_modules",
    "src": "src",
    "test": "test"
  },
  "engines": {
    "node": ">=20.19.2",
    "npm": ">=10.8.2",
    "vscode": "1.100.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://dylarcher.github.io/cssman-cli",
  "keywords": [
    "state",
    "management",
    "cli",
    "library",
    "npm",
    "javascript"
  ],
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "name": "cssman-cli",
  "optionalDependencies": {
    "@types/chrome": "0.0.326",
    "@types/vscode": "1.100.0",
    "c8": "10.1.3",
    "glob": "11.0.2",
    "rimraf": "6.0.1"
  },
  "packageManager": "npm@10.8.2",
  "peerDependencies": {
    "typescript": "5.8.3",
    "yargs-parser": "22.0.0"
  },
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dylarcher/cssman-cli.git"
  },
  "scripts": {
    "build": "tsc --declaration --declarationMap false --emitDeclarationOnly false --outDir dist --declarationDir dist/types --moduleResolution node --module esnext --target esnext --sourceMap true --inlineSourceMap true && npm run bundle:types",
    "bundle:types": "dts-bundle-generator -o dist/types.d.ts src/**/*.ts",
    "clean": "npm run clean:dist && npm run clean:libs",
    "clean:dist": "rimraf dist lib test/coverage .vscode-test types",
    "clean:libs": "rimraf node_modules package-lock.json .cache/npm",
    "format": "biome format --write --config-path .github/configs/biome.json src && markdownlint --config .github/configs/.markdownlintrc -- **/*.md",
    "lint": "biome lint --apply --config-path .github/configs/biome.json src && yamllint -- -c .github/configs/yamllint.yaml .github/**/*.yml",
    "test": "c8 --config .github/configs/.c8rc.json node --test"
  },
  "type": "module",
  "version": "0.1.0"
}
